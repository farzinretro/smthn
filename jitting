--Functions
gravity.MouseButton1Click:Connect(function()

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16821.4355, 1.23623896, 176.729843, 0, 0, -1, 0, 1, 0, 1, 0, 0)
	wait(2)
	game.Players.LocalPlayer.Character.Chest:Remove()
	game.Players.LocalPlayer.Character.Leg1:Remove()
	game.Players.LocalPlayer.Character.Leg2:Remove()
	wait(0.5)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-8003.87305, 1.69301081, 167.027954, 0, 0, -1, 0, 1, 0, 1, 0, 0)
	wait(0.5)

	game.Players.LocalPlayer.Character.Head.BillboardGui.TextLabel.Text = ("YOUR FAULT")
	game.Players.LocalPlayer.Character.Head.BillboardGui.TextLabel.TextColor3 = Color3.fromRGB(0,0,0)
	game.Players.LocalPlayer.Character.Head.BillboardGui.TextLabel.TextStrokeColor3 = Color3.fromRGB(255,0,0)

	--Appearance

	game.Players.LocalPlayer.Character.Headz.Eyes.Color = Color3.fromRGB(255,0,0)
	game.Players.LocalPlayer.Character.Headz.Eyes.Name = ("E1")
	game.Players.LocalPlayer.Character.Headz.Eyes.Color = Color3.fromRGB(255,0,0)
	game.Players.LocalPlayer.Character.Headz.Eyes.Name = ("E2")
	game.Players.LocalPlayer.Character.Headz.Eyes.Name = ("HeadCrack")
	game.Players.LocalPlayer.Character.Headz.Eyes:Remove()
	game.Players.LocalPlayer.Character.Leg1.Union.Name = ("L1")
	game.Players.LocalPlayer.Character.Leg1.Union.Name = ("L2")
	game.Players.LocalPlayer.Character.Leg1.Union.Color = Color3.fromRGB(255,255,255)
	game.Players.LocalPlayer.Character.Leg2.Union.Name = ("L1")
	game.Players.LocalPlayer.Character.Leg2.Union.Name = ("L2")
	game.Players.LocalPlayer.Character.Leg2.Union.Color = Color3.fromRGB(255,255,255)
	game.Players.LocalPlayer.Character.Headz.Teeth:Remove()
	game.Players.LocalPlayer.Backpack.Bone:Remove()
	game.Players.LocalPlayer.Backpack["Blue Bone"]:Remove()
	game.Players.LocalPlayer.Backpack["Trio Blaster"]:Remove()
	game.Players.LocalPlayer.Backpack["Giant Trio Blaster"]:Remove()
	game.Players.LocalPlayer.Backpack["Knife Throw"]:Remove()
	game.Players.LocalPlayer.Backpack["Knife Surround"]:Remove()
	game.Players.LocalPlayer.Backpack["Knife Blast"].Name = ("Attack_Slash")
	game.Players.LocalPlayer.Backpack["Attack_Slash"].ToolTip = ("VHS Sans Slash")
	wait(1)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-30178.6602, 0.943609297, -85.2601318, 0, 0, -1, 0, 1, 0, 1, 0, 0)
	wait(0.5)
	game.Players.LocalPlayer.Backpack["Unrivaled Defense"].Name = ("VHS Heal")
	game.Players.LocalPlayer.Backpack["Axe"]:Remove()
	game.Players.LocalPlayer.Backpack["Sickness"].Name = ("Hack")
	--Behavior
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50


	--Loop

	while true do
		game.Players.LocalPlayer.Character.Head.BillboardGui.Frame1.BackgroundColor3 = Color3.fromRGB(0,0,0)
		wait(0.1)
		game.Players.LocalPlayer.Character.Headz.E1.Color = Color3.fromRGB(0,0,0)
		game.Players.LocalPlayer.Character.Headz.E2.Color = Color3.fromRGB(0,0,0)
		wait(0.1)
		game.Players.LocalPlayer.Character.Headz.E1.Color = Color3.fromRGB(255,0,0)
		game.Players.LocalPlayer.Character.Headz.E2.Color = Color3.fromRGB(255,0,0)
	end

end)


