		de.Color = Color3.fromRGB(85,255,0)
		wait(1)
		d.Color = Color3.fromRGB(0,255,255)
		de.Color = Color3.fromRGB(255,0,0)
		wait(1)
		d.Color = Color3.fromRGB(255,255,0)
		de.Color = Color3.fromRGB(22,99,255)

	end

end)

